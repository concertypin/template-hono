{
    "name": "template-hono",
    "private": true,
    "license": "Apache-2.0",
    "engines": {
        "node": ">=22"
    },
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "vite build",
        "prebuild": "pnpm run cf-typegen",
        "preview": "pnpm run build && vite preview",
        "deploy": "pnpm run build && wrangler deploy",
        "cf-typegen": "wrangler types --env-interface CloudflareBindings",
        "postinstall": "pnpm run cf-typegen && pnpm exec simple-git-hooks",
        "format": "node --experimental-strip-types node_modules/prettier/bin/prettier.cjs --write ."
    },
    "dependencies": {
        "@hono/zod-openapi": "^1.1.4",
        "@scalar/hono-api-reference": "^0.9.24",
        "hono": "^4.10.4",
        "hono-openapi": "^1.1.1",
        "zod": "^4.1.12"
    },
    "devDependencies": {
        "@cloudflare/vite-plugin": "^1.14.0",
        "cross-env": "^10.1.0",
        "lint-staged": "^16.2.6",
        "prettier": "^3.6.2",
        "simple-git-hooks": "^2.13.1",
        "typescript": "^5.9.3",
        "vite": "^6.4.1",
        "wrangler": "^4.46.0"
    },
    "packageManager": "pnpm@10.16.1+sha512.0e155aa2629db8672b49e8475da6226aa4bdea85fdcdfdc15350874946d4f3c91faaf64cbdc4a5d1ab8002f473d5c3fcedcd197989cf0390f9badd3c04678706"
